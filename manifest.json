{
    "manifest_version": 2,
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "permissions": ["*://*/*"],
    "content_scripts": [
        {
            "matches": ["*://lichess.org/*"],
            "js": ["main.js"],
            "run_at": "document_start"
        }
    ]
}
