{
    "manifest_version": 2,
    "name": "CHEatSS",
    "version": "1.0",
    "description": "Stockfish Cheat Engine for lichess.org",
    "permissions": ["*://*/*"],
    "content_scripts": [
        {
            "matches": ["*://lichess.org/*"],
            "js": ["main.js"],
            "run_at": "document_start"
        }
    ]
}
